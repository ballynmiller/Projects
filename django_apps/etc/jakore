upstream jakore{
	server unix:///tmp/run/jakore.socket;
}
server{
	listen 80;
	server_name jakore.ballynmiller.com;

	location ~* \.(jpg|jpeg|js){
		expires 365d;
	}

	location /uploads{
		alias /uploads/;
	}

	location /assets{
		alias {ASSETS_PATH};
	}

	location / {
		uwsgi_pass jakore;
		include /etc/nginx/uwsgi_params;
	}
}
