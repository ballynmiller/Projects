upstream jakore{
	server unix:///tmp/run/jakore.socket;
}
server{
	listen 80;
	server_name jakore.com www.jakore.com;

	location ~* \.(jpg|jpeg|js){
		expires 365d;
	}

	location /assets{
		alias {ASSETS_PATH};
	}

	location /static{
		alias /usr/local/lib/python2.7/dist-packages/django/contrib/admin/static;
	}

	location /uploads{
		alias /uploads/;
	}

	location / {
		uwsgi_pass jakore;
		include /etc/nginx/uwsgi_params;
	}
}
